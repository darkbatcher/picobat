#: ../dos9/lang/Dos9_Help.c:34
#,
msgid ""
"Create an alias on a command.\n"
"Usage: ALIAS [/f] alias=command\n"
msgstr ""
"Create an alias on a command.\n"
"Usage: ALIAS [/f] alias=command\n"

#: ../dos9/lang/Dos9_Help.c:38
msgid ""
"Change current working directory.\n"
"Usage: CD  [[/d] path]\n"
"       CHDIR [[/d] path]\n"
msgstr ""
"Change current working directory.\n"
"Usage: CD  [[/d] path]\n"
"       CHDIR [[/d] path]\n"

#: ../dos9/lang/Dos9_Help.c:43
msgid ""
"Clear console's screen\n"
"Usage: CLS\n"
msgstr ""
"Clear console's screen\n"
"Usage: CLS\n"

#: ../dos9/lang/Dos9_Help.c:47
msgid ""
"Change console's color to the given code.\n"
"Usage: COLOR [code]\n"
msgstr ""
"Change console's color to the given code.\n"
"Usage: COLOR [code]\n"

#: ../dos9/lang/Dos9_Help.c:51
msgid ""
"Copy file(s) to another location.\n"
"Usage: COPY [/-Y | /Y] [/A[:]attributes] source destination\n"
msgstr ""
"Copy file(s) to another location.\n"
"Usage: COPY [/-Y | /Y] [/A[:]attributes] source destination\n"

#: ../dos9/lang/Dos9_Help.c:55
msgid ""
"Call a script or a label from the current context.\n"
"Usage: CALL [/e] [file] [:label] [parameters ...]\n"
msgstr ""
"Call a script or a label from the current context.\n"
"Usage: CALL [/e] [file] [:label] [parameters ...]\n"

#: ../dos9/lang/Dos9_Help.c:60
msgid ""
"Delete file(s).\n"
"Usage: DEL [/P] [/F] [/S] [/Q] [/A[:]attributes] name\n"
"       ERASE [/P] [/F] [/S] [/Q] [/A[:]attributes] name\n"
msgstr ""
"Usage: DEL [/P] [/F] [/S] [/Q] [/A[:]attributes] name\n"
"       ERASE [/P] [/F] [/S] [/Q] [/A[:]attributes] name\n"

#: ../dos9/lang/Dos9_Help.c:65
msgid ""
"List files in a directory and in subdirectories.\n"
"Usage: DIR [/A[:]attributes] [/S] [/B] [path]\n"
msgstr ""
"List files in a directory and in subdirectories.\n"
"Usage: DIR [/A[:]attributes] [/S] [/B] [path]\n"

#: ../dos9/lang/Dos9_Help.c:69
msgid ""
"Print a message, or enable/disable commande echoing.\n"
"Usage: ECHO [OFF|ON]\n"
"       ECHO text\n"
"       ECHO.text\n"
msgstr ""
"Print a message, or enable/disable commande echoing.\n"
"Usage: ECHO [OFF|ON]\n"
"       ECHO text\n"
"       ECHO.text\n"

#: ../dos9/lang/Dos9_Help.c:75
#, c-format
msgid ""
"Perform commands against items (files, strings, command output).\n"
"Usage: FOR %%A IN (string) DO (\n"
"          :: some commands\n"
"       )\n"
"\n"
"       FOR /F [options] %%A IN (object) DO (\n"
"          :: some commands\n"
"       )\n"
"\n"
"       FOR /L %%A IN (start,increment,end) DO (\n"
"          :: some commands\n"
"       )\n"
msgstr ""
"Perform commands against items (files, strings, command output).\n"
"Usage: FOR %%A IN (string) DO (\n"
"          :: some commands\n"
"       )\n"
"\n"
"       FOR /F [options] %%A IN (object) DO (\n"
"          :: some commands\n"
"       )\n"
"\n"
"       FOR /L %%A IN (start,increment,end) DO (\n"
"          :: some commands\n"
"       )\n"

#: ../dos9/lang/Dos9_Help.c:87
msgid ""
"Go to a label in a command script.\n"
"Usage: GOTO [:]label [file]\n"
msgstr ""
"Go to a label in a command script.\n"
"Usage: GOTO [:]label [file]\n"

#: ../dos9/lang/Dos9_Help.c:91
msgid ""
"Perform commands on some conditions.\n"
"Usage: IF [/i] [NOT] string1==string2 (\n"
"          :: code to be ran\n"
"       )\n"
"\n"
"       IF [/i] string1 cmp string2\n"
"          :: code to be ran\n"
"       )\n"
"\n"
"       IF [NOT] [DEFINED | EXIST | ERRORLEVEL] object (\n"
"          :: code to be ran\n"
"       )\n"
msgstr ""
"Perform commands on some conditions.\n"
"Usage: IF [/i] [NOT] string1==string2 (\n"
"          :: code to be ran\n"
"       )\n"
"\n"
"       IF [/i] string1 cmp string2\n"
"          :: code to be ran\n"
"       )\n"
"\n"
"       IF [NOT] [DEFINED | EXIST | ERRORLEVEL] object (\n"
"          :: code to be ran\n"
"       )\n"

#: ../dos9/lang/Dos9_Help.c:103
msgid ""
"Create the given directory.\n"
"Usage: MD directory\n"
"       MKDIR directory\n"
msgstr ""
"Usage: MD directory\n"
"       MKDIR directory\n"

#: ../dos9/lang/Dos9_Help.c:109
msgid ""
"Wait for a keystroke from the user.\n"
"Usage: PAUSE\n"
msgstr ""
"Wait for a keystroke from the user.\n"
"Usage: PAUSE\n"

#: ../dos9/lang/Dos9_Help.c:114
msgid ""
"Removes a directory.\n"
"Usage: RMDIR [/S] [/Q] directory\n"
"       RD [/S] [/Q] directory\n"
msgstr ""
"Removes a directory.\n"
"Usage: RMDIR [/S] [/Q] directory\n"
"       RD [/S] [/Q] directory\n"

#: ../dos9/lang/Dos9_Help.c:119
msgid ""
"Renames a file.\n"
"Usage: REN file name.ext\n"
"       RENAME file name.ext\n"
msgstr ""
"Renames a file.\n"
"Usage: REN file name.ext\n"
"       RENAME file name.ext\n"

#: ../dos9/lang/Dos9_Help.c:124
msgid ""
"Introduce a comment.\n"
"Usage: REM comment\n"
msgstr ""
"Introduce a comment.\n"
"Usage: REM comment\n"

#: ../dos9/lang/Dos9_Help.c:129
msgid ""
"Set an environment variable.\n"
"Usage: SET variable=content\n"
"       SET /a[[:][i|f]] variable=expression\n"
"       SET /p variable=question\n"
msgstr ""
"Set an environment variable.\n"
"Usage: SET variable=content\n"
"       SET /a[[:][i|f]] variable=expression\n"
"       SET /p variable=question\n"

#: ../dos9/lang/Dos9_Help.c:135
msgid ""
"Set options of the Dos9 command prompt.\n"
"Usage: SETLOCAL [ENABLEDELAYEDEXPANSION | DISABLEDELAYEDEXPANSION] "
"[ENABLEFLOATS | DISABLEFLOATS] [CMDLYCORRECT | CMDLYINCORRECT]\n"
msgstr ""
"Set options of the Dos9 command prompt.\n"
"Usage: SETLOCAL [ENABLEDELAYEDEXPANSION | DISABLEDELAYEDEXPANSION] "
"[ENABLEFLOATS | DISABLEFLOATS] [CMDLYCORRECT | CMDLYINCORRECT]\n"

#: ../dos9/lang/Dos9_Help.c:139
msgid ""
"Set console's title.\n"
"Usage: TITLE title\n"
msgstr ""
"Set console's title.\n"
"Usage: TITLE title\n"

#: ../dos9/lang/Dos9_Help.c:143
msgid ""
"Print a file.\n"
"Usage: TYPE [file]\n"
"       MORE [file]\n"
msgstr ""
"Print a file.\n"
"Usage: TYPE [file]\n"
"       MORE [file]\n"

#: ../dos9/lang/Dos9_Help.c:148
msgid ""
"Exit from the command prompt.\n"
"Usage: EXIT [/b code]\n"
msgstr ""
"Exit from the command prompt.\n"
"Usage: EXIT [/b code]\n"

#: ../dos9/lang/Dos9_Help.c:152
msgid ""
"Change positions of command-line parameters.\n"
"Usage: SHIFT [/start_number | /s[:]start_number] [/d[:]displacement]\n"
msgstr ""
"Change positions of command-line parameters.\n"
"Usage: SHIFT [/start_number | /s[:]start_number] [/d[:]displacement]\n"

